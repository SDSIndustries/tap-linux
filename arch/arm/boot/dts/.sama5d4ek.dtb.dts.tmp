        2 9 1 (0 << 0)
        2 10 1 (0 << 0)
        2 11 1 (0 << 0)
        2 12 1 (0 << 0)>;
     };
    };
    spi0 {
     pinctrl_spi0: spi0-0 {
      atmel,pins =
       <2 0 1 (0 << 0)
        2 1 1 (0 << 0)
        2 2 1 (0 << 0)
       >;
     };
    };

    ssc0 {
     pinctrl_ssc0_tx: ssc0_tx {
      atmel,pins =
       <1 27 2 (0 << 0)
        1 31 2 (0 << 0)
        1 28 2 (0 << 0)>;
     };

     pinctrl_ssc0_rx: ssc0_rx {
      atmel,pins =
       <1 26 2 (0 << 0)
        1 30 2 (0 << 0)
        1 29 2 (0 << 0)>;
     };
    };

    ssc1 {
     pinctrl_ssc1_tx: ssc1_tx {
      atmel,pins =
       <2 19 2 (0 << 0)
        2 20 2 (0 << 0)
        2 21 2 (0 << 0)>;
     };

     pinctrl_ssc1_rx: ssc1_rx {
      atmel,pins =
       <2 24 2 (0 << 0)
        2 22 2 (0 << 0)
        2 23 2 (0 << 0)>;
     };
    };

    usart2 {
     pinctrl_usart2: usart2-0 {
      atmel,pins =
       <1 4 2 (0 << 0)
        1 5 2 (1 << 0)
       >;
     };
     pinctrl_usart2_rts: usart2_rts-0 {
      atmel,pins = <1 11 2 (0 << 0)>;
     };
     pinctrl_usart2_cts: usart2_cts-0 {
      atmel,pins = <1 3 2 (0 << 0)>;
     };
    };
    usart3 {
     pinctrl_usart3: usart3-0 {
      atmel,pins =
       <4 16 2 (0 << 0)
        4 17 2 (1 << 0)
       >;
     };
    };
    usart4 {
     pinctrl_usart4: usart4-0 {
      atmel,pins =
       <4 26 2 (0 << 0)
        4 27 2 (1 << 0)
       >;
     };
     pinctrl_usart4_rts: usart4_rts-0 {
      atmel,pins = <4 28 2 (0 << 0)>;
     };
     pinctrl_usart4_cts: usart4_cts-0 {
      atmel,pins = <4 0 3 (0 << 0)>;
     };
    };
   };

   rtc@fc0686b0 {
    compatible = "atmel,at91rm9200-rtc";
    reg = <0xfc0686b0 0x30>;
    interrupts = <1 4 7>;
   };

   aic: interrupt-controller@fc06e000 {
    #interrupt-cells = <3>;
    compatible = "atmel,sama5d4-aic";
    interrupt-controller;
    reg = <0xfc06e000 0x200>;
    atmel,external-irqs = <56>;
   };
  };

  vdec0: vdec@00300000 {
   compatible = "on2,g1";
   reg = <0x00300000 0x1000>;
   interrupts = <19 4 4>;
   status = "disabled";
  };

  usb0: gadget@00400000 {
   #address-cells = <1>;
   #size-cells = <0>;
   compatible = "atmel,at91sam9rl-udc";
   reg = <0x00400000 0x100000
          0xfc02c000 0x4000>;
   interrupts = <47 4 2>;
   status = "disabled";

   ep0 {
    reg = <0>;
    atmel,fifo-size = <64>;
    atmel,nb-banks = <1>;
   };

   ep1 {
    reg = <1>;
    atmel,fifo-size = <1024>;
    atmel,nb-banks = <3>;
